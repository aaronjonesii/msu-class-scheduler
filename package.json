{
  "name": "msu-class-scheduler",
  "version": "1.0.1",
  "scripts": {
    "build:development": "ng build --configuration development",
    "build:production": "ng build",
    "dev": "firebase emulators:exec --import=./seed --export-on-exit --ui \"npm run start:development\"",
    "emulators": "firebase emulators:start --import=./seed --export-on-exit",
    "lint": "ng lint",
    "ng": "ng",
    "serve:ssr:msu-class-scheduler": "node dist/msu-class-scheduler/server/server.mjs",
    "start:development": "ng serve",
    "start:production": "ng serve --configuration production",
    "test": "ng test",
    "test:headless": "ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "watch": "ng build --watch --configuration development",
    "deploy:preview": "firebase hosting:channel:deploy preview"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.7",
    "@angular/cdk": "^18.2.7",
    "@angular/common": "^18.2.7",
    "@angular/compiler": "^18.2.7",
    "@angular/core": "^18.2.7",
    "@angular/fire": "^18.0.1",
    "@angular/forms": "^18.2.7",
    "@angular/material": "^18.2.7",
    "@angular/platform-browser": "^18.2.7",
    "@angular/platform-browser-dynamic": "^18.2.7",
    "@angular/platform-server": "^18.2.7",
    "@angular/router": "^18.2.7",
    "@angular/service-worker": "^18.2.7",
    "@angular/ssr": "^18.2.7",
    "angular-eslint": "^18.1.0",
    "express": "^4.21.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.7.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.7",
    "@angular/cli": "^18.2.7",
    "@angular/compiler-cli": "^18.2.7",
    "@types/express": "^5.0.0",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^22.7.4",
    "angular-eslint": "18.3.1",
    "eslint": "^9.11.1",
    "jasmine-core": "~5.3.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.5.1",
    "typescript-eslint": "8.8.0"
  }
}
